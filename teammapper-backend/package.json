{
  "name": "teammapper-backend",
  "version": "0.0.4",
  "description": "teammapper backend to work online as a team on mindmaps",
  "author": "b310 digital gmbh",
  "private": true,
  "license": "MIT",
  "engines": {
    "npm": "~9.5.0",
    "node": "~18.14.2"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "client": "cd ../ && npm start --prefix teammapper-frontend",
    "dev": "concurrently \"nest start --watch\" \"npm run client\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev:typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --dataSource src/data-source.ts",
    "dev:typeorm:migrate": "npm run dev:typeorm migration:run",
    "dev:data:maps:cleanup": "ts-node ./src/jobs/deleteMaps.job.ts",
    "dev:data:maps:seed": "ts-node ./src/jobs/seedMapData.job.ts",
    "prod:data:maps:cleanup": "node ./dist/jobs/deleteMaps.job.js",
    "prod:typeorm:migrate": "typeorm migration:run --dataSource dist/data-source.js"
  },
  "dependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/platform-socket.io": "^9.3.9",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.3.9",
    "cache-manager": "3.6.3",
    "dotenv": "10.0.0",
    "npm-check-updates": "^16.3.7",
    "pg": "^8.6.0",
    "pq": "^0.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "4.6.1",
    "typeorm": "^0.3.14"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.3.9",
    "@types/cache-manager": "3.4.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.21",
    "@types/jest": "28.1.4",
    "@types/node": "^18.14.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "concurrently": "6.2.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.4",
    "eslint-plugin-nestjs": "1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-typeorm": "0.0.19",
    "jest": "28.1.2",
    "jest-jasmine2": "28.1.2",
    "prettier": "^2.2.1",
    "socket.io-client": "^4.6.1",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*.*spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "testRunner": "jest-jasmine2",
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
