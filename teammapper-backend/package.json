{
  "name": "teammapper-backend",
  "version": "0.0.1",
  "description": "teammapper backend to work online as a team on mindmaps",
  "author": "b310 digital gmbh",
  "private": true,
  "license": "MIT",
  "engines": {
    "npm": "~8.3.1",
    "node": "~16.14.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "client": "cd ../ && npm start --prefix teammapper-frontend",
    "dev": "concurrently \"nest start --watch\" \"npm run client\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:prod:migrate": "typeorm migration:run --config dist/ormconfig.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.3",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.0.7",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.0.7",
    "cache-manager": "3.6.0",
    "dotenv": "10.0.0",
    "pg": "^8.6.0",
    "pq": "^0.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "4.2.0",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cache-manager": "3.4.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.21",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "concurrently": "6.2.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "15.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-nestjs": "1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-typeorm": "0.0.19",
    "jest": "^27.3.1",
    "jest-jasmine2": "27.3.1",
    "prettier": "^2.2.1",
    "socket.io-client": "^4.1.3",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.7",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testRunner": "jest-jasmine2",
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
